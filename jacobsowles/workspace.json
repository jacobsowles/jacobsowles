{
  "version": 2,
  "projects": {
    "jacobsowles": {
      "root": "apps/jacobsowles",
      "sourceRoot": "apps/jacobsowles/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/jacobsowles",
            "index": "apps/jacobsowles/src/index.html",
            "main": "apps/jacobsowles/src/main.tsx",
            "polyfills": "apps/jacobsowles/src/polyfills.ts",
            "tsConfig": "apps/jacobsowles/tsconfig.app.json",
            "assets": [
              "apps/jacobsowles/src/favicon.ico",
              "apps/jacobsowles/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/jacobsowles/src/environments/environment.ts",
                  "with": "apps/jacobsowles/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "jacobsowles:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "jacobsowles:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/jacobsowles/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/jacobsowles"
          ],
          "options": {
            "jestConfig": "apps/jacobsowles/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "jacobsowles-e2e": {
      "root": "apps/jacobsowles-e2e",
      "sourceRoot": "apps/jacobsowles-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/jacobsowles-e2e/cypress.json",
            "devServerTarget": "jacobsowles:serve",
            "tsConfig": "apps/jacobsowles-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jacobsowles:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/jacobsowles-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "seat-time": {
      "root": "apps/seat-time",
      "sourceRoot": "apps/seat-time/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/seat-time",
            "index": "apps/seat-time/src/index.html",
            "main": "apps/seat-time/src/main.tsx",
            "polyfills": "apps/seat-time/src/polyfills.ts",
            "tsConfig": "apps/seat-time/tsconfig.app.json",
            "assets": [
              "apps/seat-time/src/favicon.ico",
              "apps/seat-time/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/seat-time/src/environments/environment.ts",
                  "with": "apps/seat-time/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "seat-time:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "seat-time:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/seat-time/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/seat-time"
          ],
          "options": {
            "jestConfig": "apps/seat-time/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "seat-time-e2e": {
      "root": "apps/seat-time-e2e",
      "sourceRoot": "apps/seat-time-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/seat-time-e2e/cypress.json",
            "devServerTarget": "seat-time:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "seat-time:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/seat-time-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/ui/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui"
          ],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui",
            "config": {
              "configFolder": "libs/ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-e2e": {
      "root": "apps/ui-e2e",
      "sourceRoot": "apps/ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-e2e/cypress.json",
            "devServerTarget": "ui:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ui:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/ui-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "jacobsowles"
}